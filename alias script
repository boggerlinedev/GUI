Render.Position = UDim2.new(0.474381059, 0, 0.0241448693, 0)
Render.Size = UDim2.new(0, 136, 0, 426)

TextLabel_3.Parent = Render
TextLabel_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
